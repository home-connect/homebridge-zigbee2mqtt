{
  "pluginAlias": "home-connect-homebridge-zigbee2mqtt",
  "pluginType": "platform",
  "singular": true,
  "schema": {
    "type": "object",
    "properties": {
      "name": {
        "title": "MQTT Address",
        "type": "string",
        "default": "mqtt://127.0.0.1",
        "minLength": 1,
        "required": true
      },
      "port": {
        "title": "Port",
        "type": "integer",
        "default": 1883,
        "maximum": 65535,
        "required": true
      }
    }
  }
}
